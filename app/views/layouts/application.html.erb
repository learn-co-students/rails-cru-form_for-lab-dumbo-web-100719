<!DOCTYPE html>
<html>
<head>
  <title>BeatsByRails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
      <%= link_to 'All Artists', artists_path %>
      <%= link_to 'All Songs', songs_path %>
      <%= link_to 'All Genres', genres_path %>
  </nav>

<%= yield %><br>
  <nav>
      <%= link_to 'New Artist', new_artist_path %><br>
      <%= link_to 'New Song', new_song_path %><br>
      <%= link_to 'New Genre', new_genre_path %><br>
  </nav>
  
</body>
</html>
