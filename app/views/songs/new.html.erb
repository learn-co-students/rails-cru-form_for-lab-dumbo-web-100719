<h2>Create a new Song!</h2>
<%= form_for @song do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :artist%>
    <%= f.collection_select(:artist_id, Artist.all, :id, :name ) %>

    <%= f.label :genre%>
    <%= f.collection_select(:genre_id, Genre.all, :id, :name ) %>

    <%= f.submit %>
<% end %>